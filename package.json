{
  "name": "fitness-coach-desktop",
  "version": "1.0.0",
  "main": "src/main.js",
  "type": "commonjs",
  "description": "",
  "author": "",
  "license": "ISC",
"scripts": {
  "dev": "concurrently -k \"npm:dev:react\" \"npm:dev:electron:wait\"",
  "dev:react": "vite --config src/ui/vite.config.mjs --open",
  "dev:electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
  "dev:electron:wait": "wait-on http://localhost:5173 && npm run dev:electron",
  "build:react": "vite build --config src/ui/vite.config.mjs",
  "start": "electron .",
  "rebuild": "electron-rebuild -f -w better-sqlite3",
  "postinstall": "electron-rebuild -f -w better-sqlite3",
  "test": "echo \"Error: no test specified\" && exit 1"
},
  "keywords": [],
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "ical-generator": "^9.0.0",
    "nodemailer": "^7.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.0",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
